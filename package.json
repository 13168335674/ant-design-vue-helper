{
		"name": "vscode-faicomponent-helper",
		"displayName": "faicomponent helper",
		"description": "A vscode extension for faicomponent",
		"version": "1.0.6",
		"publisher": "adi",
		"repository": {
			"type": "git",
			"url": "https://github.com/13168335674/vscode-faicomponent-helper"
		},
		"engines": {
				"vscode": "^1.30.0"
		},
		"icon": "antdv.png",
		"categories": [
			"Snippets",
			"Other"
		],
		"activationEvents": [
				"onLanguage:html",
				"onLanguage:vue",
				"onCommand:antdv-helper.search"
		],
		"contributes": {
				"commands": [
						{
								"command": "antdv-helper.search",
								"title": "antdv-helper.search"
						}
				],
				"keybindings": [
						{
								"command": "antdv-helper.search",
								"key": "shift+cmd+i",
								"when": "editorTextFocus"
						}
				],
				"snippets": [
						{
								"language": "javascript",
								"path": "./snippets/antdv.json"
						}
				],
				"configuration": {
						"type": "object",
						"title": "Antdv Helper Configuration",
						"properties": {
								"antdv-helper.indent-size": {
										"type": "number",
										"default": 2,
										"description": "Indentation size of snippets"
								},
								"antdv-helper.quotes": {
										"type": "string",
										"default": "double"
								},
								"antdv-helper.link-url": {
									"type": "string",
									"default": "http://fa.docs.aaa.cn"
							}
						}
				}
		},
		"main": "./out/extension.js",
		"scripts": {
				"vscode:prepublish": "sh build.sh",
				"compile": "sh build.sh",
				"watch": "sh build.sh --watch",
				"postinstall": "node ./node_modules/vscode/bin/install",
				"test": "npm run compile && node ./node_modules/vscode/bin/test"
		},
		"devDependencies": {
				"typescript": "^3.1.4",
				"vscode": "^1.1.25",
				"tslint": "^5.8.0",
				"@types/node": "^8.10.25",
				"@types/mocha": "^2.2.42"
		},
		"dependencies": {
				"pretty": "^2.0.0",
				"shelljs": "^0.8.3"
		}
}
